<body class="bg-silver-300">
    <div class="content">
        <div class="brand">
            
        </div>
        <form id="login-form" action="javascript:func()" method="post">
            <h2 class="login-title">Cadastrar Empresa</h2>
			<div class="form-group">
                <div class="input-group-icon right">
                    <div class="input-icon"><i class="fa fa-address-book-o"></i></div>
                    <input class="form-control" type="text" id="nome_empresa" name="nome_empresa" placeholder="NOME DA EMPRESA" autocomplete="off">
                </div>
            </div>
            <div class="form-group">
                <div class="input-group-icon right">
                    <div class="input-icon"><i class="fa fa-id-card-o"></i></div>
                    <input class="form-control" type="text" id="cnpj" name="cnpj" placeholder="CNPJ" autocomplete="off">
                </div>
            </div>
			<div class="form-group">
                <div class="input-group-icon right">
                    <div class="input-icon"><i class="fa fa-envelope"></i></div>
                    <input class="form-control" type="text" id="email" name="email" placeholder="EMAIL" autocomplete="off">
                </div>
            </div>
			<div class="form-group">
                <div class="input-group-icon right">
                    <div class="input-icon"><i class="fa fa-address-book-o"></i></div>
                    <input class="form-control" type="text" id="nome" name="nome" placeholder="NOME" autocomplete="off">
                </div>
            </div>
            <div class="form-group">
                <div class="input-group-icon right">
                    <div class="input-icon"><i class="fa fa-lock font-16"></i></div>
                    <input class="form-control" type="password" id="senha" name="senha" placeholder="Senha">
                </div>
            </div>
            <div class="form-group">
                <button class="btn btn-info btn-block" type="submit" id="cadEmpresa">Cadastrar</button>
            </div>
            <div class="text-center">
                
				<a class="color-blue" href="/login">Voltar</a>
            </div>
        </form>
    </div>
    <div class="sidenav-backdrop backdrop"></div>
    <div class="preloader-backdrop">
        <div class="page-preloader">Loading</div>
    </div>
	<script>
           swal("Mensagem", {
