
<div class="modal fade " id="modalProfile" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<form id="formulario" action="javascript:func()" method="post">
	<div class="modal-dialog  modal-md" role="document">
		<div class="modal-content">
		<div class="modal-header">
			<h5 class="modal-title" id="exampleModalLabel">Meus Dados</h5>
			<button type="button" class="close" data-dismiss="modal" aria-label="Fechar">
			<span aria-hidden="true">&times;</span>
			</button>
		</div>
		<div class="modal-body">
			
				<div class="col-sm-12">
					<h4 class="card-inside-title ">Empresa</h4>
					<div class="form-group">
						<div class="form-line">
							<input type="text" class="form-control" placeholder="" id="nome_empresa" value="<%= nome_empresa%>" required />
						</div>
					</div>
				</div>
				<div class="col-sm-12">
					<h4 class="card-inside-title ">CNPJ</h4>
					<div class="form-group">
						<div class="form-line">
							<input type="text" class="form-control" placeholder="" id="cnpj_empresa" value="<%= cnpj_empresa%>" required />
						</div>
					</div>
				</div>
				
				<div class="col-sm-12">
					<h4 class="card-inside-title ">Nome</h4>
					<div class="form-group">
						<div class="form-line">
							<input type="text" class="form-control" placeholder="" id="nome_user" value="<%= nome_user %>"required />
						</div>
					</div>
				</div>
				<div class="col-sm-12">
					<h4 class="card-inside-title ">Email</h4>
					<div class="form-group">
						<div class="form-line">
							<input type="text" class="form-control" placeholder="" id="email_user" value="<%= email_user %>"required />
						</div>
					</div>
				</div>
				<div class="col-sm-12">
					<h4 class="card-inside-title ">Senha</h4>
					<div class="form-group">
						<div class="form-line">
							<input type="password" class="form-control" placeholder="" id="senha_user"  />
						</div>
					</div>
				</div>
			
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
			<button class="btn btn-primary" type="submit" id="atualizarDadosEmpresaUsusario"><i class="fa fa-fw fa-lg fa-check-circle"></i>Atualizar Dados</button>&nbsp;&nbsp;&nbsp;
		</div>
		</div>
	</div>
	</form>
</div>